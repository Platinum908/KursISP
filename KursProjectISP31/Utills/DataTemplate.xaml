<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:KursProjectISP31.ViewModel"
                    xmlns:view="clr-namespace:KursProjectISP31.View">
    <DataTemplate DataType="{x:Type vm:TablesViewModel}">
        <view:TablesView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:MenuViewModel}">
        <view:MenuView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:BookingViewModel}">
        <view:BookingView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:OrderViewModel}">
        <view:OrderView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:ReportsViewModel}">
        <view:ReportsView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:OrderTrackingViewModel}">
        <view:OrderTrackingView/>
    </DataTemplate>
</ResourceDictionary>